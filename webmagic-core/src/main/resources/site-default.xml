<?xml version="1.0" encoding="UTF-8"?>
<app desc="采集程序配置文件"
	xmlns="http://www.qysudu.com/" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.qysudu.com/ site.xsd ">
	<site desc="httpclient参数、http协议头">
		<userAgent>Mozilla/5.0 (X11; Ubuntu; Linux x86_64; rv:50.0) Gecko/20100101 Firefox/50.0</userAgent>
		<charset>UTF-8</charset>
		<sleepTime>5000</sleepTime>
		<retryTimes>0</retryTimes>
		<cycleRetryTimes>0</cycleRetryTimes>
		<retrySleepTime>1000</retrySleepTime>
		<timeOut>5000</timeOut>
		<useGzip>true</useGzip>
		<Header>
			<key-value  name="User-Agent" value="Mozilla/5.0 (X11; Ubuntu; Linux x86_64; rv:50.0) Gecko/20100101 Firefox/50.0" />
			<key-value  name="Accept" value="text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8" />
			<key-value  name="Accept-Encoding" value="gzip, deflate" />
			<key-value  name="Accept-Language" value="zh-CN,zh;q=0.8,en-US;q=0.5,en;q=0.3" />
			<key-value  name="Cache-Control" value="no-cache" />
			<key-value  name="Connection" value="keep-alive" />
			<key-value  name="Content-Type" value="application/x-www-form-urlencoded; charset=UTF-8" />
		</Header>
	</site>
</app>